{% extends 'admin/index.html.twig' %}
  {% block content %}

      <div class="content-wrapper" >
          <!-- Content Header (Page header) -->
          <section class="content-header" >
              <h1 >
                  Городской транспорт

              </h1 >
              <ol class="breadcrumb" >
                  <li ><a href="#" ><i class="fa fa-dashboard" ></i > Home</a ></li >
                  <li class="active" >Городской транспорт</li >
              </ol >
          </section >

          <!-- Main content -->
          <section class="content" >
              <!-- /.row -->
              <!-- Main row -->

              <div class="row" >
                  <div class="col-md-6" >
                      <div class="box box-warning" >

                          <div class="box-body" >
                              <form role="form" >
                                  
                                  <div class="form-group" >
                                      <label >Автобусы</label >
                                      <input type="text" class="form-control" placeholder="номера через запятую" >
                                  </div >
                                 <div class="form-group" >
                                      <label >Тролейбусы</label >
                                      <input type="text" class="form-control" placeholder="номера через запятую" >
                                  </div >
                                  <div class="form-group" >
                                      <label >Трамваи</label >
                                      <input type="text" class="form-control" placeholder="номера через запятую" >
                                  </div >
                                  <div class="form-group" >
                                      <label >Информация о парковке</label >
                                      <textarea class="form-control" rows="3" placeholder="Enter ..."></textarea>
                                  </div >



                                  <div class="box-footer" >

                                      <button type="submit" class="btn btn-info " >Создать</button >
                                  </div >
                              </form >
                          </div >
                      </div >
                  </div >
              </div >


          </section >

          <!-- /.content -->
  {% endblock %}

          {% block jsinline %}
          <script>
              $(function () {
                  //Date picker
                  $('#datepicker').datepicker({
                      autoclose: true
                  })
                  $('#datepicker2').datepicker({
                      autoclose: true
                  });
                  $('.select2').select2()
              })

          </script>
          {% endblock %}