{% extends 'base.html.twig' %}

{% block title %}Plus{% endblock %}

{% block body %}
    <h1>Plus</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ plus.id }}</td>
            </tr>
            <tr>
                <th>Plus</th>
                <td>{{ plus.plus }}</td>
            </tr>
            <tr>
                <th>Active</th>
                <td>{{ plus.active ? 'Yes' : 'No' }}</td>
            </tr>
            <tr>
                <th>Sort</th>
                <td>{{ plus.sort }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('plus_index') }}">back to list</a>

    <a href="{{ path('plus_edit', {'id': plus.id}) }}">edit</a>

    {{ include('plus/_delete_form.html.twig') }}
{% endblock %}
